{
  "name": "clinikal/openemr",
  "description": "Emergency Medicine vertical of clinikal project (fork of OpenEMR)",
  "authors": [
    {
      "name": "Clinikal team"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:MohGovIL/Clinical-Backend.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:MohGovIL/Clinical-composer-installers.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:MohGovIL/Clinical-MOH-IL.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "git@github.com:MohGovIL/Clinicak-Vertical-addictions.git"
    }
  ],
  "require": {
    "clinikal/composer-installers-clinikal-extender": "dev-master",
    "clinikal/clinikal-backend": "<CLINIKAL_BACKEND_BRANCH>",
    "clinikal/moh-il-backend": "<CLINIKAL_MOH_IL_BRANCH>",
    "clinikal/vertical-addictions": "<VERTICAL_BRANCH>"
  },
  "extra": {
    "installer-types": [
      "clinikal-vertical",
      "clinikal-react"
    ],
    "installer-paths": {
    }
  },
  "config":{
    "clinikal-env": "dev",
    "install-name": "<INSTALL_NAME>"
  }
}
